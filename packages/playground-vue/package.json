    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "tailwindcss": "^3.3.3",
    "vue": "^3.2.27",
    "vue-flatpickr-component": "^9.0.5",
    "vue-router": "^4.0.0"
  },
  "devDependencies": {
    "@floating-ui/vue": "^1.0.2",
    "@vitejs/plugin-vue": "^3.0.3",
    "vite": "^3.0.0"
