    "@vue/compiler-sfc": "3.0.1",
    "@vue/test-utils": "^2.0.0-beta.7",
    "vite": "^1.0.0-rc.4",
    "vue": "^3.0.0",
    "vue-router": "^4.0.0-beta.13"
